---
interface Props {
  width: string,
  height: string
}

const { width, height } = Astro.props;

---

<svg
  width={width}
  height={height}
  viewBox="0 0 48 48"
  xmlns="http://www.w3.org/2000/svg"
>
  <title>code</title>
  <linearGradient id="gradient" x1="100%" y1="0%" x2="0%" y2="100%">
    <stop offset="0%"></stop>
    <stop offset="50%"></stop>
    <stop offset="100%"></stop>
  </linearGradient>
  <g id="Layer_2" data-name="Layer 2">
    <g id="invisible_box" data-name="invisible box">
      <rect width="48" height="48" fill="none"></rect>
    </g>
    <g id="Q3_icons" data-name="Q3 icons">
      <path
        d="M22,35h-.5a2,2,0,0,1-1.4-2.5l4-17.9a2,2,0,1,1,3.8,1l-4,18A1.9,1.9,0,0,1,22,35Z"
        fill="url(#gradient)"></path>
      <path
        d="M17,31a2,2,0,0,0,1.3-3.5L14,24l4.3-3.5a2,2,0,1,0-2.6-3l-6,5a2,2,0,0,0,0,3l6,5A1.9,1.9,0,0,0,17,31Z"
        fill="url(#gradient)"></path>
      <path
        d="M31,31a2,2,0,0,1-1.3-3.5L34,24l-4.3-3.5a2,2,0,0,1,2.6-3l6,5a2,2,0,0,1,0,3l-6,5A1.9,1.9,0,0,1,31,31Z"
        fill="url(#gradient)"></path>
      <path
        d="M44,6H4A2,2,0,0,0,2,8V40a2,2,0,0,0,2,2H44a2,2,0,0,0,2-2V8A2,2,0,0,0,44,6ZM42,38H6V10H42Z"
        fill="url(#gradient)"></path>
    </g>
  </g>
</svg>

<style>
  stop:nth-child(1) {
    stop-color: var(--gradient-color-2);
  }
  stop:nth-child(2) {
    stop-color: var(--gradient-color-1);
  }
  stop:nth-child(3) {
    stop-color: var(--gradient-color-2);
  }
</style>
